<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.messedup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MessedUp">
        <activity android:name=".dashboard.NewActivity"></activity>
        <activity android:name=".dashboard.UploadNotice">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".dashboard.HomeActivity" />
        </activity>
        <activity android:name=".dashboard.ManageNotice" />
        <activity android:name=".dashboard.AdminLogin" />
        <activity android:name=".dashboard.HomeActivity" />
        <activity android:name=".notice.ShowNotice" />
        <activity android:name=".notice.AddNotice" />
        <activity android:name=".notice.NoticeActivity" />
        <activity android:name=".BookActivity" />
        <activity android:name=".SubjectActivity" />
        <activity android:name=".BranchActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>